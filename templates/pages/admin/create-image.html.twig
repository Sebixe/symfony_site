{% extends 'layouts/main.html.twig' %}

{% block title %}Ajouter une image{% endblock title %}
{% block content %}

        <div class="container">
            <div class="about-us-content">
                <div class="gallery-jobs">
                    <div class="row">
                        <div class="col-md-12 col-sm-6">
                            <div class="single-package-item">
                <img src="{{ asset('assets/images/packages/register.jpg') }}" alt="package-place">
                <div class="single-package-item-txt">
                    <h3>Ajouter une nouvelle image de fond</h3>

                    <div class="row">
                        </br>
                        {{ form_start(imagesForm) }}
                        {{ form_row(imagesForm.title) }}
                        {{ form_row(imagesForm.level) }}
                        {{ form_row(imagesForm.location) }}
                        {{ form_errors(imagesForm.location) }}
                    </div>
                        </br>
                    {{ form_row(imagesForm.submit) }}
                    {{ form_rest(imagesForm) }}

                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
</section>

{% endblock content %}

