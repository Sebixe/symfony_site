                                                            <label class="list-group-item">
                                                                    {{ dungeons.level }}.
                                                                        {{ dungeons.title }}
                                                                    <input type="checkbox" value="1" class="margin-left float-right" name="{{ dungeons.id }}">
                                                                <a href="{{ dungeons.position }}"  class="disapear float-right eorzeadb_link" target="_blank" rel="noopener noreferrer">
                                                                    Quête d'accès
                                                                </a>

                                                                </label>
                                                            {% else %}
