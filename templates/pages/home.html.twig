                                                            <h4>{{ user.server }}</h4>

                                                        </div><!--/.single-tab-select-box-->

                                                    </div><!--/.col-->
                                                    {{ form_start(LevelForm) }}
                                                    <div class="col-lg-4 col-md-4 col-sm-12">
                                                        <div class="text-center single-tab-select-box">
                                                         <div class="lvl-up"> {{ form_row(LevelForm.submit) }}</div>
                                                            <img class="image-display" src="{{ asset('assets/images/offer/execute_limit_break.png') }}"/>
