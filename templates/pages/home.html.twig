{% extends "layouts/main.html.twig" %}

{% block title %}La grande compagnie d'Eorzea{% endblock title %}
{% block content %}

        <div class="container">

            {% if is_granted('ROLE_USER') -%}
                <div class="col-md-12 col-sm-6">
                        <div class="col-md-12">
                            <div class="single-travel-boxes">
                                <div id="desc-tabs" class="desc-tabs">

                                    <ul class="nav nav-tabs" role="tablist">

                                        <li role="presentation" class="active">
                                            <a href="#hotels" aria-controls="hotels" role="tab" data-toggle="tab">
                                                <i class="fa fa-gamepad"></i>
                                                Donjons & Défis
                                            </a>
                                        </li>

                                        <li role="presentation">
                                            <a href="#flights" aria-controls="flights" role="tab" data-toggle="tab">
                                                <i class="fa fa-question-circle"></i>
                                                Que faire ?
                                            </a>
                                        </li>

                                        <li role="presentation">
                                            <a href="#tours" aria-controls="tours" role="tab" data-toggle="tab">
                                                <i class="fa fa-user"></i>
                                                Profil
                                            </a>
                                        </li>

                                    </ul>

                                    <!-- Tab panes -->
                                    <div class="tab-content">
                                        <div role="tabpanel" class="tab-pane fade in" id="tours">
                                            <div class="tab-para">

                                                <div class="row">
                                                    <div class="col-lg-4 col-md-4 col-sm-12">
                                                        <div class="text-center single-tab-select-box">

                                                            <img src=" {{user.job}}" width="92" height="92";>
                                                            <h4><br/>{{ user.username }}</h4>

                                                        </div><!--/.single-tab-select-box-->
                                                    </div><!--/.col-->
                                                    <div class="col-lg-4 col-md-4 col-sm-12">
                                                        <div class="text-center single-tab-select-box">

                                                            <img class="image-display" src="{{ asset('assets/images/offer/return.png') }}"/>
